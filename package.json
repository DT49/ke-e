{
  "name": "ke-e",
  "version": "0.3.1",
  "description": "a property-based testing framwork.",
  "repository": {
    "type": "git",
    "url": "git://github.com/hychen/ke-e.git"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/hychen/ke-e/issues"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "test": "mocha",
    "cov": "./node_modules/.bin/nyc npm test",
    "build": "npm run build:nodejs && npm run build:browser",
    "build:nodejs": "./node_modules/.bin/babel src -d lib",
    "build:browser": "./node_modules/.bin/webpack",
    "clean": "rm -rf ./lib",
    "lint": "./node_modules/.bin/eslint src/**/**/*.js test",
    "lintgit": "./tools/eslint",
    "doc": "./node_modules/.bin/jsdoc -c ./.jsdoc.json",
    "ci-deploy": "npm run doc && npm run build:browser && gh-pages -d dist"
  },
  "keywords": [
    "test",
    "quickcheck"
  ],
  "author": "Chen Hsin-Yi",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.8",
    "eslint": "~2.2.0",
    "gh-pages": "^0.11.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.0",
    "jsverify": "^0.7.3",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^6.1.1",
    "pre-commit": "^1.1.3",
    "shelljs": "^0.7.4",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "lodash": "^4.6.1",
    "random-js": "^1.0.8"
  },
  "pre-commit": [
    "lintgit"
  ]
}
